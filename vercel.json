{
    "version" : 2,
    "builds" : [
        {
            "src" : "app04/wsgi.py",
            "use" : "@vercel/python",
            "config" : { "maxLambdaSize" : "50mb", "runtime" : "python3.9" }
        }
    ],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "app04/wsgi.py"
        }
    ]
}
